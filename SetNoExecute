# Cvs does not maintain permissions well, so use this script to set them
chmod -x AUTHORS
chmod -x BUGS
chmod -x COPYING
chmod -x ChangeLog
chmod -x INSTALL
chmod -x NEWS
chmod -x README
chmod -x omh/*.omh
chmod -x omh/getstarted/*.omh
chmod -x omh/latex/*.omh
chmod -x omh/getstarted/w3c_home.gif
chmod -x omh/image/*.gif
chmod -x omh/image/*.jpg
chmod -x omh/image/*.png
chmod -x OMhelp/*.gif
chmod -x OMhelp/*.wrd
chmod -x OMhelp/*.xsl
chmod -x src/*.c
chmod -x src/*.h
chmod -x src/*.h
chmod -x src/*.sln
chmod -x src/*.vcproj
