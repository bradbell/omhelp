sed -e "/^[^#]/d" -e "s/^[#] *//" < CvsCommit > cvs.log
cvs commit -F cvs.log \
	CvsCommit       \
	omh/whatsnew.omh \
	omh/firefox.omh \
	omh/framelink.omh \
	src/omhelp.y 
